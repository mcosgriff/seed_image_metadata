{
    "seedVersion": "1.0.0",
    "job": {
        "name": "tiff-metadata-job",
        "jobVersion": "0.0.1",
        "packageVersion": "1.0.0",
        "title": "Tiff Metadata",
        "description": "Get metadata for tiff file",
        "maintainer": {
            "name": "Matthew Cosgriff",
            "email": "mcosgrif@ball.com"
        },
        "timeout": 10,
        "resources": {
            "scalar": [
                {
                    "name": "cpus",
                    "value": 0.1
                },
                {
                    "name": "mem",
                    "value": 16.0
                },
                {
                    "name": "disk",
                    "value": 1.0,
                    "inputMultiplier": 1.0
                },
                {
                    "name": "sharedMem",
                    "value": 128
                }
            ]
        },
        "interface": {
            "command": "${TIFF_FILE} ${OUTPUT_DIR}",
            "inputs": {
                "files": [
                    {
                        "name": "TIFF_FILE",
                        "required": true,
                        "mediaTypes": [
                            "image/tiff"
                        ],
                        "multiple": false,
                        "partial": false
                    }
                ]
            },
            "outputs": {
                "files": [
                    {
                        "name": "OUTPUT_FILE",
                        "mediaType": "text/json",
                        "pattern": "*_output*.json",
                        "multiple": false,
                        "required": true
                    }
                ]
            },
            "mounts": [
                {
                    "name": "MOUNT_BIN",
                    "path": "/usr/bin/",
                    "mode": "ro"
                },
                {
                    "name": "MOUNT_TMP",
                    "path": "/tmp/",
                    "mode": "rw"
                }
            ]
        },
        "errors": [
            {
                "code": 1,
                "name": "data-error",
                "title": "No Input Provided",
                "description": "No input files have been given. No data to add.",
                "category": "data"
            }
        ]
    }
}